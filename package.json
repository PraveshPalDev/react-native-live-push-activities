{
  "name": "react-native-live-push-activities",
  "version": "1.0.0",
  "description": "Easy-to-use iOS Live Activities implementation for React Native CLI and Expo with real-time push notifications",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "files": [
    "src",
    "lib",
    "ios",
    "android",
    "react-native-live-push-activities.podspec"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example/ios && pod install",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "live-activities",
    "dynamic-island",
    "lock-screen",
    "push-notifications",
    "widget",
    "expo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PraveshPalDev/react-native-live-push-activities.git"
  },
  "author": "PraveshPalDev <praveshkumar@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PraveshPalDev/react-native-live-push-activities/issues"
  },
  "homepage": "https://github.com/PraveshPalDev/react-native-live-push-activities#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.25",
    "@types/react-native": "^0.72.5",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-builder-bob": "^0.23.1",
    "release-it": "^16.2.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community"
    ]
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
